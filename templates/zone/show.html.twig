{% extends 'base.html.twig' %}

{% block title %}Zone{% endblock %}

{% block body %}
    <h1>Zone</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ zone.id }}</td>
            </tr>
            <tr>
                <th>Zone</th>
                <td>{{ zone.zone }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('zone_index') }}">Retour à la liste</a>

    <a href="{{ path('zone_edit', {'id': zone.id}) }}">Modifier</a>

    {{ include('zone/_delete_form.html.twig') }}
{% endblock %}
